# Anthropic API Key - Required for Claude AI integration
# Get from https://console.anthropic.com/keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here
CLAUDE_API_KEY=your_claude_api_key_here

# Supabase Configuration - Required for authentication and database
# Get from your Supabase project settings -> API
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Server Port (optional, defaults to 3000)
PORT=3000

# Cache TTL in seconds (optional, defaults to 3600 = 1 hour)
CACHE_TTL=3600

# Log Level (optional, defaults to 'info')
# Options: error, warn, info, debug
LOG_LEVEL=info
